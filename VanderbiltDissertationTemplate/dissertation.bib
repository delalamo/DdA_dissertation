keywords = {Electron paramagnetic resonance (EPR),Electron-electron double resonance,Macromolecular movements,Membrane proteins,Spin labeling,Structure determination},
number = {January},
pages = {419--446},
pmid = {22404592},
title = {{DEER distance measurements on proteins}},
