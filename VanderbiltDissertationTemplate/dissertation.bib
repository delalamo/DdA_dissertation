issn = {00280836},
journal = {Nature},
keywords = {Animals,Antibodies,Antibody Specificity,Antibody Specificity: immunology,Apoproteins,Apoproteins: chemistry,Apoproteins: genetics,Apoproteins: immunology,Apoproteins: metabolism,Bacterial Proteins,Bacterial Proteins: chemistry,Bacterial Proteins: genetics,Bacterial Proteins: immunology,Bacterial Proteins: metabolism,Binding Sites,Crystallography,Immunoglobulin Fab Fragments,Immunoglobulin Fab Fragments: immunology,Ions,Ions: chemistry,Models,Molecular,Monoclonal,Monoclonal: immunology,Movement,Protein Conformation,Sodium,Sodium: chemistry,Sodium: metabolism,Structure-Activity Relationship,Substrate Specificity,X-Ray},
