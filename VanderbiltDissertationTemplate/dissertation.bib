publisher = {Nature Publishing Group},
title = {{X-ray structures of Drosophila dopamine transporter in complex with nisoxetine and reboxetine}},
volume = {22},
year = {2015}
}
@article{Baranov2015,
abstract = {The non-universality of the genetic code is now widely appreciated. Codes differ between organisms, and certain genes are known to alter the decoding rules in a site-specific manner. Recently discovered examples of decoding plasticity are particularly spectacular. These examples include organisms and organelles with disruptions of triplet continuity during the translation of many genes, viruses that alter the entire genetic code of their hosts and organisms that adjust their genetic code in response to changing environments. In this Review, we outline various modes of alternative genetic decoding and expand existing terminology to accommodate recently discovered manifestations of this seemingly sophisticated phenomenon.},
author = {Baranov, Pavel V and Atkins, John F and Yordanova, Martina M},
