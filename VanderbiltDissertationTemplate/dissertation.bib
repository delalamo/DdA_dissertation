pages = {149--155},
pmid = {24316739},
title = {{Protonation drives the conformational switch in the multidrug transporter LmrP.}},
volume = {10},
year = {2014}
}
@article{Pannier2000,
abstract = {A four-pulse version of the pulse double electronâelectron resonance (DEER) experiment is presented, which is designed for the determination of interradical distances on a nanoscopic lengthscale. With the new pulse sequence electronâelectron couplings can be studied without dead-time artifacts, so that even broad distributions of electronâelectron distances can be characterized. A version of the experiment that uses a pulse train in the detection period exhibits improved signal-to-noise ratio. Tests on two nitroxide biradicals with known length indicate that the accessible range of distances extends from about 1.5 to 8 nm. The four-pulse DEER spectra of an ionic spin probe in an ionomer exhibit features due to probe molecules situated both on the same and on different ion clusters. The former feature provides information on the cluster size and is inaccessible with previous methods.},
author = {Pannier, M and Veit, S and Godt, A and Jeschke, G and Spiess, H W},
doi = {https://doi.org/10.1006/jmre.1999.1944},
issn = {1090-7807},
journal = {Journal of Magnetic Resonance},
keywords = {DEER,EPR spectroscopy,biradical,distance measurement,electron spin echo},
number = {2},
pages = {331--340},
title = {{Dead-Time Free Measurement of DipoleâDipole Interactions between Electron Spins}},
url = {https://www.sciencedirect.com/science/article/pii/S1090780799919444},
volume = {142},
year = {2000}
}
