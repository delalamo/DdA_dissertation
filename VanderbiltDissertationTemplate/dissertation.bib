pages = {1630--1642},
pmid = {16880288},
title = {{A comprehensive structure-based alignment of prokaryotic and eukaryotic neurotransmitter/Na+ symporters (NSS) aids in the use of the LeuT structure to probe NSS structure and function}},
url = {http://molpharm.aspetjournals.org/cgi/content/abstract/70/5/1630%5Cnpapers3://publication/uuid/DA3F5CFE-AF48-4DCF-AF1E-B772C7C31D16},
volume = {70},
year = {2006}
}
