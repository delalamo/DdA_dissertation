booktitle = {Journal of Biomolecular NMR},
doi = {10.1007/BF02192796},
issn = {09252738},
keywords = {Activation domain B,NMR solution structure,Procarboxypeptidase B,Protein structure,X-ray crystal structure},
number = {1},
