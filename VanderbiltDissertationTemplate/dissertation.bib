journal = {Journal of Magnetic Resonance},
keywords = {Bi-label distance,Bi-label distance distribution,Double Electron Electron Resonance (DEER),Freezing perturbation,Pulse dipolar spectroscopy (PDS),Rapid freeze quench,T4 lysozyme},
pages = {69--77},
pmid = {22341208},
publisher = {Elsevier Inc.},
title = {{Effect of freezing conditions on distances and their distributions derived from Double Electron Electron Resonance (DEER): A study of doubly-spin-labeled T4 lysozyme}},
