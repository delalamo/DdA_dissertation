author = {Tsuchiya, Hirotoshi and Doki, Shintaro and Takemoto, Mizuki and Ikuta, Tatsuya and Higuchi, Takashi and Fukui, Keita and Usuda, Yoshihiro and Tabuchi, Eri and Nagatoishi, Satoru and Tsumoto, Kouhei and Nishizawa, Tomohiro and Ito, Koichi and Dohmae, Naoshi and Ishitani, Ryuichiro and Nureki, Osamu},
doi = {10.1038/nature17991},
