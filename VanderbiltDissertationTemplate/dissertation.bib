isbn = {1548-7105 (Electronic) 1548-7091 (Linking)},
issn = {15487105},
journal = {Nature Methods},
number = {1},
pages = {71--73},
pmid = {27819658},
publisher = {Nature Publishing Group},
title = {{CHARMM36m: An improved force field for folded and intrinsically disordered proteins}},
