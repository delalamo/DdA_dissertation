journal = {Nature Structural and Molecular Biology},
keywords = {Bacterial Proteins,Chemotaxis,Crystallography, X-Ray,Cytoplasm,Dimerization,Electron Spin Resonance Spectroscopy,Membrane Proteins,Methyl-Accepting Chemotaxis Proteins,Models, Molecular,Protein Binding,Protein Folding,Protein Kinases,Protein Structure, Quaternary,Signal Transduction,Thermotoga maritima,chemistry,cytology,enzymology,genetics,metabolism},
language = {eng},
