keywords = {Bacterial acid resistance,Cellular localization,Escherichia coli,Glutamate decarboxylase,Pyridoxal 5â²-phosphate},
number = {16},
pages = {4027--4037},
pmid = {12912902},
title = {{Crystal structure and functional analysis of Escherichia coli glutamate decarboxylase}},
url = {http://emboj.embopress.org/cgi/doi/10.1093/emboj/cdg403},
volume = {22},
year = {2003}
}
@article{Geier2013,
abstract = {The Large-neutral Amino Acid Transporter 1 (LAT-1)-a sodiumindependent exchanger of amino acids, thyroid hormones, and prescription drugs-is highly expressed in the blood-brain barrier and various types of cancer. LAT-1 plays an important role in cancer development as well as in mediating drug and nutrient delivery across the blood-brain barrier, making it a key drug target. Here, we identify four LAT-1 ligands, including one chemically novel substrate, by comparative modeling, virtual screening, and experimental validation. These results may rationalize the enhanced brain permeability of two drugs, including the anticancer agent acivicin. Finally, two of our hits inhibited proliferation of a cancer cell line by distinct mechanisms, providing useful chemical tools to characterize the role of LAT-1 in cancer metabolism.},
